<!DOCTYPE html>
<html>
<head>
  <title>Dynport</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render '/layouts/userauth' -%>
<% if user_signed_in? %>
<%= render '/layouts/nav' -%>
<% end %>

<%= flash[:notice] %>
<%= flash[:alert] %>

<%= yield %>

</body>
</html>
